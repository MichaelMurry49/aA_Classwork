<h1>Logen In</h1>

<form action="/session" method="POST">
    <%= auth_token %>
    <label>Username:
        <input type="text" name="user[username]">
    </label>

    <label>Password:
        <input type="password" name="user[password]">
    </label>

    <button type="submit">Log In</button>
</form>